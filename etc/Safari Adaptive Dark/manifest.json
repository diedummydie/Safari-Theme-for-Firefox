      "toolbar_bottom_separator": "#000000",
      "toolbar_vertical_separator": "#000000ff",
      "tab_selected": "#3e3e3e",
      "sidebar": "#262626",
      "ntp_background": "#262626",
      "sidebar_text": "#dedede",
      "ntp_text": "#dedede",
      "popup_highlight": "#3992f1",
