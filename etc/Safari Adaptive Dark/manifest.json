{
  "manifest_version": 2,

  "name": "Safari Adapt Dark",
  "version": "2019.1102",
  "description": "Even better with Safari-Theme-for-Firefox: https://git.io/JeBLr",

  "icons": {
    "48": "dark48.png",
    "96": "dark96.png",
    "256": "dark256.png"
  },

  "theme": {
    "colors": {
      "toolbar": "#484848",
      "toolbar_text": "#ffffff",
      "frame": "#2b2b2b",
      "tab_background_text": "#c7c7c7",
      "toolbar_field": "#6f6f6f",
